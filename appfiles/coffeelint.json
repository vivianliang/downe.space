{
  "no_trailing_whitespace": {
    "level": "error"
  },
  "arrow_spacing": {
    "level": "error"
  },
  "line_endings": {
    "value": "unix",
    "level": "error"
  },
  "max_line_length": {
    "value": 100,
    "level": "error"
  },
  "newlines_after_classes": {
    "value": "2",
    "level": "error"
  },
  "no_empty_param_list": {
    "level": "error"
  },
  "no_unnecessary_fat_arrows": {
    "level": "error"
  },
  "empty_constructor_needs_parens": {
    "level": "error"
  },
  "no_interpolation_in_single_quotes": {
    "level": "error"
  },
  "no_unnecessary_double_quotes": {
    "level": "error"
  },
  "no_debugger": {
    "level": "error"
  },
  "ensure_comprehensions": {
    "level": "error"
  },
  "conditional_modifiers": {
    "module": "coffeelint-conditional-modifiers",
    "level": "error"
  },
  "forbidden_keywords": {
    "module": "coffeelint-forbidden-keywords",
    "forbidden": {
      "yes": "true",
      "no": "false",
      "on": "true",
      "off": "false",
      "unless": "if not"
    }
  },
  "use_strict": {
    "module": "coffeelint-use-strict",
    "level": "error",
    "requireGlobal": true
  },
  "no_implicit_returns": {
    "module": "coffeelint-no-implicit-returns"
  }
}
